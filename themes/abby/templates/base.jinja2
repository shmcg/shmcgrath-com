  <body>
    <header id="headerBase">
      <p id="headerBaseSitename"><a href="{{ SITEURL }}/">{{ SITENAME }}</a></p>
      {% if SITESUBTITLE %}
        <p id="headerBaseSitesubtitle">{{ SITESUBTITLE }}</p>
      {% endif %}
      <nav id="navBase">
        <ul id="navBaseMenu">
          {% if DISPLAY_PAGES_ON_MENU %}
            {% for p in pages %}
              <li{% if p == page %} class="active"{% endif %}><a href="{{ SITEURL }}/{{ p.url }}">{{ p.title }}</a></li>
            {% endfor %}
          {% endif %}

          {% for title, link in MENUITEMS %}
              <li><a href="{{ link }}">{{ title }}</a></li>
          {% endfor %}

          {% if DISPLAY_CATEGORIES_ON_MENU %}
            {% for cat, null in categories %}
              <li{% if cat == category %} class="active"{% endif %}><a href="{{ SITEURL }}/{{ cat.url }}">{{ cat }}</a></li>
            {% endfor %}
          {% endif %}
        </ul>
      </nav>
    </header>

    <main id="mainBase">
      {% block content %}
      {% endblock %}
    </main>

    <footer id="footerBase" class="footer">

      <div id="backTop" class="footerDiv">
      <p><a href="#top">Back to Top</a></p>
      </div>

      <div id="socialIcons" class="footerDiv">
        <ul id="social-icons-list">
          <li><a href="https://github.com/shmcgrath"><img src="{{ SITEURL }}/img/githubr.png" alt=""></a></li>
          <li><a href="https://www.linkedin.com/in/mcgrathsh"><img src="{{ SITEURL }}/img/linkedinr.png" alt=""></a></li>
          <li><a href="mailto:sarah@shmcgrath.com"><img src="{{ SITEURL }}/img/mailr.png" alt=""></a></li>
          <li><a href="https://twitter.com/mcgrath_sh"><img src="{{ SITEURL }}/img/twitterr.png" alt=""></a></li>
          <li><a href="https://www.reddit.com/user/shmcg/"><img src="{{ SITEURL }}/img/redditr.png" alt=""></a></li>
          <li><a href="{{ SITEURL }}/feeds/all.atom.xml"><img src="{{ SITEURL }}/img/rssr.png" alt=""></a></li>
        </ul>
      </div>

      <div id="blogArchive" class="footerDiv">
      <ul>
        {% if ARCHIVES_SAVE_AS %}
          <li><a href="{{ SITEURL }}/{{ ARCHIVES_SAVE_AS }}">Archives</a></li>
        {% endif %}
        {% if TAGS_SAVE_AS %}
